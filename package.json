{
  "name": "luma-ui",
  "version": "0.0.0",
  "private": false,
  "workspaces": [
    "packages/*",
    "packages/settings/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:ui": "turbo run build --filter @luma/ui",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "tsup": "^6.6.3",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
